<UserControl x:Class="GeneFinder.Views.PedazoAnotacionControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GeneFinder.Views"
             mc:Ignorable="d" 
             d:DesignHeight="50" d:DesignWidth="75">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Rectangle Grid.Column="0" x:Name="annottationOne" Grid.Row="1" Fill="Orange" Margin="0,2,0,0" Visibility="Visible"/>
        <Rectangle Grid.Column="1" x:Name="annotationTwo" Grid.Row="1" Fill="Orange" Margin="0,2,0,0" Visibility="Visible"/>
        <Rectangle Grid.Column="2" x:Name="annotationThree" Grid.Row="1" Fill="Orange" Margin="0,2,0,0" Visibility="Visible"/>
    </Grid>
</UserControl>
